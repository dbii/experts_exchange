<div class="row">
  <div class="col-md-6">
    <h1>Create Expert</h1>
    <%= form_for(
            @expert,
            url: experts_path,
            ) do |f| %>
      <div class="form-group mt-3">
        <%= f.label :name%>
        <%= f.text_field :name, class: 'form-control'  %><br/>
      </div>

      <div class="form-group mt-3">
        <%= f.label :url, 'URL' %>
        <%= f.text_field :url, class: 'form-control'  %><br/>
      </div>

      <%= f.submit 'Save', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
